{
  "name": "rest_api",
  "version": "1.0.0",
  "description": "Test project for a REST API using Node.js, Express, and TypeScript",
  "main": "dist/server.js",
  "author": "Ray Caddick",
  "license": "MIT",
  "scripts": {
    "build:tsoa": "tsoa spec-and-routes",
    "build:spectacle": "node spectacle.build.js",
    "build:typescript": "tsc --project tsconfig.build.json",
    "build": "npm-run-all --sequential build:*",
    "start:build": "npm run build",
    "start:run": "node dist/server.js",
    "start": "npm-run-all --sequential start:*",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test": "jest --verbose",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:run": "npm run typeorm migration:run -- -d ./src/config/database/mysql/mysql.config.ts",
    "migrations:revert": "npm run typeorm migration:revert -- -d ./src/config/database/mysql/mysql.config.ts",
    "migrations:generate": "ts-node ./src/config/database/mysql/migrations/migrations.cli.ts",
    "migrations:seed": "ts-node -r tsconfig-paths/register ./src/config/database/mysql/seeding/data.seed.ts"
  },
  "_moduleAliases": {
    "@root": "./dist",
    "@application": "./dist/application",
    "@controllers": "./dist/application/controllers",
    "@services": "./dist/application/services",
    "@config": "./dist/config",
    "@domain": "./dist/domain",
    "@user": "./dist/domain/user",
    "@product": "./dist/domain/product",
    "@infrastructure": "./dist/infrastructure",
    "@logger": "./dist/application/services/logger",
    "@entities": "./dist/infrastructure/entities",
    "@middleware": "./dist/infrastructure/middleware",
    "@repositories": "./dist/infrastructure/repositories",
    "@utils": "./dist/utils"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.2.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "global": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.0",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "winston": "^3.12.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.29",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  }
}
